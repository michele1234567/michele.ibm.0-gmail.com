{
   "browser_action": {
      "default_icon": "icon.png",
      "default_popup": "popup.html"
   },
   "description": "QuikNote is an extension that allows you to take a note while capturing the current url. View anywhere when logged into Chrome",
   "key": "",
   "manifest_version": 2,
   "name": "QuikNote",
   "permissions": [
      "activeTab",
      "tabs",
      "https://ajax.googleapis.com/",
      "storage",
      "alarms",
      "notifications"
   ],
   "update_url": "https://clients2.google.com/service/update2/crx",
   "version": "1.2",
   "options_page": "settings.html",
   "background": {
      "scripts": [
         "note-common.js",
         "background.js"
      ],
      "persistent": false
   }
}